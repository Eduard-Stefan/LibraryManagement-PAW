@model IEnumerable<Library_Management.Models.Subsidiary>

@{
	ViewData["Title"] = "Subsidiaries";
}

<h2 class="text-center mb-5">Subsidiaries</h2>

<table class="table">
	<thead>
	<tr>
		<th>
			@Html.DisplayNameFor(model => model.Name)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Address)
		</th>
		<th></th>
	</tr>
	</thead>
	<tbody>
	@foreach (var item in Model) {
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.Name)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Address)
			</td>
		</tr>
	}
	</tbody>
</table>